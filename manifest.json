{
    "manifest_version": 2,
    "name": "HackPHS",
    "version": "0.1",
    
    "permissions": [
    "activeTab"
    ],
    
    "browser_action": {
        "default_icon": "static/img/logo.jpg",
        "default_popup": "templates/extension.html",
        "default_title": "HackPHS!"
      },
    
    "content_scripts": [
    {
       "matches": [
     "*://twitter.com/*"
       ],
       "js": ["static/scripts/content_twitter.js"]
     }
   ]
   }
